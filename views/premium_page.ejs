        <!-- <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet"> -->
				  
		<div id="Login" class="Login_form_container">
			<br><br><br><br><br><div class="container">
				  
			<div class="row">
				<div class="col-lg-6"> 
					<form id="Login_form" action="Start_chat">
						<div class="section-title">LYNN CHAT </div>
						<div class="form-group">
							<label class="label-control" for="signup_username">Username</label>
							<input type="name" class="form-control-input" id="signup_username" name="Signup_username" required/>				
						</div>
						
						<div  class="form-group">
							<label   for="room"><b>&nbsp Select a technician to contact.</b></label>
							<select class="form-control-select" name="room" id="room">
								<option value="Abel">Technician Abel</option>
								<option value="Gedion">Technician Gedion</option>
								<option value="Henock">Technician Henock</option>
								<option value="Melat">Technician Melat</option>
								<option value="Menase">Technician Menase</option>
								<option value="Rediet">Technician Rediet</option>
							</select>
						</div>
		
						<div class="form-group">
							<button type="submit" class="form-control-submit-button">Join Chat</button>
						</div>
						
					</form>
				
			</div>
			<div class="col-lg-6">
				<div class="text-container">
					<!--<div class="section-title">Login</div>-->
					<h2 class="white">Choose your technician and start chatting right away.</h2>
					<ul class="list-unstyled li-space-lg white">
						<li class="media">
							<i class="fas fa-square"></i>
							<div class="media-body">Not satisfied with the chat.</div>
						</li>
						<li class="media">
							<i class="fas fa-square"></i>
							<div class="media-body">Premium users have access to ONSITE SUPPORT</div>
						</li>
						<li class="media">
							<i class="fas fa-square"></i>
							<div class="media-body">Please request for support below.</div>
						</li>
						<a class="btn-solid-lg page-scroll" href="../requests/new">Request Support</a>
					</ul><br><br><br><br><br><br><br><br>
				</div>
			</div>
		</div>
		</div>
		</div>
